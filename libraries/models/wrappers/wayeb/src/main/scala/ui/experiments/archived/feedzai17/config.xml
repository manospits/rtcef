<configuration>
    <horizon>200</horizon>
    <order>1</order>
    <expirationTime>-1</expirationTime>
    <predictionThresholds>
        <predictionThreshold>0.1</predictionThreshold>
        <predictionThreshold>0.2</predictionThreshold>
        <predictionThreshold>0.3</predictionThreshold>
        <predictionThreshold>0.4</predictionThreshold>
        <predictionThreshold>0.5</predictionThreshold>
        <predictionThreshold>0.6</predictionThreshold>
        <predictionThreshold>0.7</predictionThreshold>
        <predictionThreshold>0.8</predictionThreshold>
        <predictionThreshold>0.9</predictionThreshold>
    </predictionThresholds>
    <maxSpreads>
        <maxSpread>10</maxSpread>
        <maxSpread>20</maxSpread>
        <maxSpread>30</maxSpread>
        <!--
        <maxSpread>40</maxSpread>
        <maxSpread>50</maxSpread>
        <maxSpread>100</maxSpread>
        <maxSpread>200</maxSpread>
        -->
    </maxSpreads>
    <resultsDir>/mnt/Warehouse/speedd/fz_generator_data/results/</resultsDir>
    <events>
        <event>
            <name>Transaction</name>
            <char>t</char>
        </event>
        <event>
            <name>IncreasingAmount</name>
            <char>i</char>
        </event>
        <event>
            <name>DecreasingAmount</name>
            <char>d</char>
        </event>
        <event>
            <name>FarAwayLocation</name>
            <char>a</char>
        </event>
        <event>
            <name>BigAmount</name>
            <char>b</char>
        </event>
        <event>
            <name>SmallAmount</name>
            <char>s</char>
        </event>
        <event>
            <name>FlashAttack</name>
            <char>f</char>
        </event>
    </events>
    <patterns>
        <pattern>
            <name>IncreasingAmounts</name>
            <sequence>tiiiiiii</sequence> <!--Allowed chars: t,i-->
            <policy>non-overlap</policy>
        </pattern>
        <pattern>
            <name>DecreasingAmounts</name>
            <sequence>tddddddd</sequence> <!--Allowed chars: t,d-->
            <policy>non-overlap</policy>
        </pattern>
        <!--
        <pattern>
            <name>FarAwayLocations</name>
            <sequence>ta</sequence>
            <policy>non-overlap</policy>
        </pattern>
        <pattern>
            <name>BigAfterSmall</name>
            <sequence>bs</sequence>
            <policy>non-overlap</policy>
        </pattern>
        -->
        <pattern>
            <name>FlashAttack</name>
            <sequence>tfffffff</sequence> <!--Allowed chars: t,f-->
            <policy>non-overlap</policy>
        </pattern>
        <pattern>
            <name>IncreasingAmountsExtra</name>
            <sequence>tiiiiiii</sequence> <!--Allowed chars: t,i,d-->
            <policy>non-overlap</policy>
        </pattern>
        <pattern>
            <name>DecreasingAmountsExtra</name>
            <sequence>tddddddd</sequence> <!--Allowed chars: t,d,i-->
            <policy>non-overlap</policy>
        </pattern>
        <pattern>
            <name>FlashAttackExtra</name>
            <sequence>tfffffff</sequence> <!--Allowed chars: t,a,f-->
            <policy>non-overlap</policy>
        </pattern>
    </patterns>
    <parameters>
        <farAwayWindow>300000</farAwayWindow>
        <bigThreshold>0.5</bigThreshold>
        <smallThreshold>400.0</smallThreshold>
        <bigAfterSmallWindow>300000</bigAfterSmallWindow>
        <flashAttackWindow>300000</flashAttackWindow>
    </parameters>
</configuration>